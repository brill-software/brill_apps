{
    "pageDescrip": "Layout",
    "rootComponent": {
        "id": "ErrorBoundary",
        "module": "react/ErrorBoundary",
        "attributes": {
            "key": "errorBoundary"
        },
        "children": [
            {
                "id": "themeProvider",
                "module": "material_ui/theme/ThemeProvider",
                "attributes": {
                    "key": "themeProvider",
                    "themeTopic": "/Themes/brill_software_theme.json"
                },
                "children": [
                    {
                        "id": "PageDiv",
                        "module": "html/Div",
                        "attributes": {
                            "key": "pageDiv",
                            "style": {
                                "minHeight": "100vh",
                                "display": "flex",
                                "flexDirection": "column"
                            }
                        },
                        "children": [
                            {
                                "id": "embedHeader",
                                "module": "material_ui/layout/EmbeddedPage",
                                "attributes": {
                                    "key": "embedHeader",
                                    "subscribeToTopic": "/Pages/header.json"
                                }
                            },
                            {
                                "id": "cmsContainer",
                                "module": "material_ui/layout/Container",
                                "attributes": {
                                    "maxWidth": "md",
                                    "fixed": false,
                                    "disableGutters": false
                                },
                                "children": [
                                    {
                                        "id": "cmsXhtml",
                                        "module": "material_ui/text/Xhtml",
                                        "attributes": {
                                            "subscribeToTopic": "/Resources/cms.xhtml"
                                        }
                                    }
                                ]
                            },
                            {
                                "id": "Spacer",
                                "module": "html/Div",
                                "attributes": {
                                    "style": {
                                        "flexGrow": 1
                                    }
                                },
                                "children": []
                            },
                            {
                                "id": "embedFooter",
                                "module": "material_ui/layout/EmbeddedPage",
                                "attributes": {
                                    "subscribeToTopic": "/Pages/footer.json"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
}