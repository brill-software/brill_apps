{
    "pageDescrip": "Demo App",
    "rootComponent": {
        "id": "ErrorBoundary",
        "module": "react/ErrorBoundary",
        "attributes": {
            "key": "errorBoundary"
        },
        "children": [
            {
                "id": "ThemeProvider",
                "module": "material_ui/theme/ThemeProvider",
                "attributes": {
                    "key": "themeProvider",
                    "themeTopic": "/Themes/brill_software_theme.json"
                },
                "children": [
                    {
                        "id": "PageDiv",
                        "module": "html/Div",
                        "attributes": {
                            "key": "pageDiv",
                            "style": {
                                "minHeight": "100vh",
                                "display": "flex",
                                "flexDirection": "column"
                            }
                        },
                        "children": [
                            {
                                "id": "embedHeader",
                                "module": "material_ui/layout/EmbeddedPage",
                                "attributes": {
                                    "key": "embedHeader",
                                    "subscribeToTopic": "/Pages/header.json"
                                }
                            },
                            {
                                "id": "demoContainer",
                                "module": "material_ui/layout/Container",
                                "attributes": {
                                    "maxWidth": "md",
                                    "fixed": false,
                                    "disableGutters": false
                                },
                                "children": [
                                    {
                                        "id": "pagePart1",
                                        "module": "material_ui/text/Xhtml",
                                        "attributes": {
                                            "subscribeToTopic": "/Resources/demo_part1.xhtml"
                                        }
                                    },
                                    {
                                        "id": "Table1",
                                        "module": "material_ui/table/DataTable",
                                        "attributes": {
                                            "title": "Employees",
                                            "size": "small",
                                            "padding": "none",
                                            "subscribeToTopic": "/Database/employee/readPage.js",
                                            "columns": [
                                                {
                                                    "name": "employee_id",
                                                    "label": "Employee Id",
                                                    "options": {
                                                        "filter": true,
                                                        "filterType": "textField"
                                                    }
                                                },
                                                {
                                                    "name": "first_name",
                                                    "label": "First Name",
                                                    "options": {
                                                        "filter": true,
                                                        "filterType": "textField"
                                                    }
                                                },
                                                {
                                                    "name": "last_name",
                                                    "label": "Last Name",
                                                    "options": {
                                                        "filter": true,
                                                        "filterType": "textField"
                                                    }
                                                },
                                                {
                                                    "name": "department",
                                                    "label": "Department",
                                                    "options": {
                                                        "filter": true,
                                                        "filterType": "textField"
                                                    }
                                                }
                                            ],
                                            "options": {
                                                "serverSide": true,
                                                "rowsPerPage": 15,
                                                "selectableRowsOnClick": true,
                                                "selectableRows": "multiple",
                                                "selectableRowsHideCheckboxes": false,
                                                "filter": true,
                                                "search": true,
                                                "print": true,
                                                "download": true,
                                                "viewColumns": true,
                                                "resizableColumns": true,
                                                "customToolbar": null,
                                                "draggableColumns": {
                                                    "enabled": true
                                                },
                                                "responsive": "vertical",
                                                "count": 45
                                            },
                                            "newRoute": "",
                                            "editRoute": "",
                                            "duplicateRoute": "",
                                            "deleteRoute": ""
                                        }
                                    },
                                    {
                                        "id": "pagePart2",
                                        "module": "material_ui/text/Xhtml",
                                        "attributes": {
                                            "subscribeToTopic": "/Resources/demo_part2.xhtml"
                                        }
                                    }
                                ]
                            },
                            {
                                "id": "Spacer",
                                "module": "html/Div",
                                "attributes": {
                                    "style": {
                                        "flexGrow": 1
                                    }
                                },
                                "children": []
                            },
                            {
                                "id": "embedFooter",
                                "module": "material_ui/layout/EmbeddedPage",
                                "attributes": {
                                    "subscribeToTopic": "/Pages/footer.json"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
}