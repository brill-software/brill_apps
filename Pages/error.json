{
    "pageDescrip": "Error page",
    "rootComponent": {
        "id": "ErrorBoundary",
        "module": "react/ErrorBoundary",
        "attributes": {},
        "children": [
            {
                "id": "themeProvider",
                "module": "material_ui/theme/ThemeProvider",
                "attributes": {
                    "themeTopic": "/Themes/error_theme.json"
                },
                "children": [
                    {
                        "id": "titleBox",
                        "module": "material_ui/layout/Box",
                        "attributes": {
                            "padding": 3,
                            "bgcolor": "error.main"
                        },
                        "children": [
                            {
                                "id": "errorHeader",
                                "module": "material_ui/text/Typography",
                                "attributes": {
                                    "variant": "h3",
                                    "text": "An error has occurred."
                                }
                            }
                        ]
                    },
                    {
                        "id": "errorTopicBox",
                        "module": "material_ui/layout/Box",
                        "attributes": {
                            "padding": 3,
                            "bgcolor": "white",
                            "condition": "return !isProduction()"
                        },
                        "children": [
                            {
                                "id": "errorTopic",
                                "module": "material_ui/text/Typography",
                                "attributes": {
                                    "variant": "h3",
                                    "text": "",
                                    "subscribeToTopic": "error.topic"
                                }
                            }
                        ]
                    },
                    {
                        "id": "errorTitleBox",
                        "module": "material_ui/layout/Box",
                        "attributes": {
                            "padding": 3,
                            "bgcolor": "white"
                        },
                        "children": [
                            {
                                "id": "errorTitle",
                                "module": "material_ui/text/Typography",
                                "attributes": {
                                    "variant": "h3",
                                    "text": "",
                                    "subscribeToTopic": "error.title"
                                }
                            }
                        ]
                    },                   
                    {
                        "id": "errorDetailBox",
                        "module": "material_ui/layout/Box",
                        "attributes": {
                            "padding": 3,
                            "bgcolor": "white",
                            "condition": "return !isProduction()"
                        },
                        "children": [
                            {
                                "id": "errorDetail",
                                "module": "material_ui/text/Typography",
                                "attributes": {
                                    "variant": "h3",
                                    "text": "",
                                    "subscribeToTopic": "error.detail"
                                }
                            }
                        ]
                    },
                    {
                        "id": "errorDetailBox",
                        "module": "material_ui/layout/Box",
                        "attributes": {
                            "padding": 5,
                            "bgcolor": "white"
                        },
                        "children": [
                            {
                                "id": "homeButton",
                                "module": "material_ui/button/LinkButton",
                                "color": "primary",
                                "attributes": {
                                    "title": "Home",
                                    "route": "/${appName}/home"
                                }
                            }         
                        ]
                    }    
                           
                ]
            }
        ]
    }
}