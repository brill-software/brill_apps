{
    "pageDescrip": "Div Component",
    "permission": "cms_user",
    "rootComponent": {
        "id": "ErrorBoundary",
        "module": "react/ErrorBoundary",
        "attributes": {},
        "children": [
            {
                "id": "ThemeProvider",
                "module": "material_ui/theme/ThemeProvider",
                "attributes": {
                    "themeTopic": "/Themes/storybook_theme_light.json",
                    "themeTopicDark": "/Themes/storybook_theme_dark.json"
                },
                "children": [
                    {
                        "id": "Module",
                        "module": "material_ui/text/Typography",
                        "attributes": {
                            "variant": "h3",
                            "gutterBottom": true,
                            "text": "material_ui/button/IconButton"
                        }
                    },
                    {
                        "id": "Attributes",
                        "module": "material_ui/text/Xhtml",
                        "attributes": {
                            "text": "<p>Creates a button from a Material-UI icon or an SVG icon.</p>\n<p>\n<table cellpadding=\"7\">\n<tr><td>iconName</td><td>The Material-UI Icon Name or the topic for an SVG icon.</td></tr>\n<tr><td>tooltip</td><td>The tooltip.</td></tr>\n<tr><td>publishToTopic</td><td>The topic to publish to when the user clicks on the icon.</td></tr>\n<tr><td>action</td><td>A string or object that is published to the publishToTopic.</td></tr>\n<tr><td>route [optional]</td><td>Location to take user when they click on the icon. e.g. /${appName}/home</td></tr>\n<tr><td>theme</td><td><code>theme.overrides.IconButton.root</code></td></tr>\n</table>\n</p>"
                        }
                    },
                    {
                        "id": "Flex Row Layout",
                        "module": "html/Div",
                        "attributes": {
                            "style": {
                                "display": "flex",
                                "flexDirection": "row"
                            }
                        },
                        "children": [
                            {
                                "id": "^Add",
                                "module": "material_ui/button/IconButton",
                                "attributes": {
                                    "iconName": "AddTwoTone",
                                    "tooltip": "Add",
                                    "publishToTopic": "comLib.IconButton.response",
                                    "action": "Add icon."
                                }
                            },
                            {
                                "id": "^Delete",
                                "module": "material_ui/button/IconButton",
                                "attributes": {
                                    "iconName": "DeleteTwoTone",
                                    "tooltip": "Delete",
                                    "publishToTopic": "comLib.IconButton.response",
                                    "action": "Delete icon."
                                }
                            },
                            {
                                "id": "^Folder",
                                "module": "material_ui/button/IconButton",
                                "attributes": {
                                    "iconName": "FolderTwoTone",
                                    "tooltip": "Folder",
                                    "publishToTopic": "comLib.IconButton.response",
                                    "action": "Folder icon.",
                                    "style": {
                                        "color": "green"
                                    }
                                }
                            },
                            {
                                "id": "^Home",
                                "module": "material_ui/button/IconButton",
                                "attributes": {
                                    "iconName": "Home",
                                    "tooltip": "Home",
                                    "publishToTopic": "comLib.IconButton.response",
                                    "action": "Home icon.",
                                    "style": {
                                        "color": "red"
                                    }
                                }
                            },
                            {
                                "id": "^Save (SVG Icon)",
                                "module": "material_ui/button/IconButton",
                                "attributes": {
                                    "iconName": "/Icons/fixed/SaveTwoTone.svg",
                                    "tooltip": "Save",
                                    "publishToTopic": "comLib.IconButton.response",
                                    "action": "Save icon.",
                                    "style": {
                                        "fill": "brown"
                                    }
                                }
                            }
                        ]
                    },
                    {
                        "id": "RelativeDiv",
                        "module": "html/Div",
                        "attributes": {
                            "style": {
                                "position": "relative"
                            }
                        },
                        "children": [
                            {
                                "id": "^Close",
                                "module": "material_ui/button/IconButton",
                                "attributes": {
                                    "iconName": "Close",
                                    "tooltip": "Close",
                                    "publishToTopic": "comLib.IconButton.response",
                                    "action": "Close icon.",
                                    "style": {
                                        "position": "absolute",
                                        "top": "5px",
                                        "right": "5px",
                                        "color": "grey"
                                    }
                                }
                            }
                        ]
                    },
                    {
                        "id": "Last Clicked",
                        "module": "material_ui/text/Typography",
                        "attributes": {
                            "variant": "body1",
                            "subscribeToTopic": "comLib.IconButton.response",
                            "style": {
                                "padding": "20px"
                            }
                        }
                    }
                ]
            }
        ]
    }
}