{
    "pageDescrip": "Div Component",
    "permission": "cms_user",
    "rootComponent": {
        "id": "ErrorBoundary",
        "module": "react/ErrorBoundary",
        "attributes": {},
        "children": [
            {
                "id": "ThemeProvider",
                "module": "material_ui/theme/ThemeProvider",
                "attributes": {
                    "themeTopic": "/Themes/storybook_theme_light.json",
                    "themeTopicDark": "/Themes/storybook_theme_dark.json"
                },
                "children": [
                    {
                        "id": "Module",
                        "module": "material_ui/text/Typography",
                        "attributes": {
                            "variant": "h3",
                            "gutterBottom": true,
                            "text": "material_ui/button/LinkButton"
                        }
                    },
                    {
                        "id": "Attributes",
                        "module": "material_ui/text/Xhtml",
                        "attributes": {
                            "text": "<p>A button, based on the Material UI Button, that routes the user to another page.\n<table cellpadding=\"7\">\n<tr><td>startIcon [optional]</td><td>Icon to place before the title. e.g. Home</td></tr>\n<tr><td>title</td><td>The title text of the Link Button.</td></tr>\n<tr><td>endIcon [optional]</td><td>Icon to place after the title.</td></tr>\n<tr><td>route</td><td>The route to take when the button is clicked. e.g. /my_app/home</td></tr>\n<tr><td>theme</td><td><code>theme.overrides.LinkButton.root</code></td></tr>\n</table>\n</p>"
                        }
                    },
                    {
                        "id": "Flex Row Layout",
                        "module": "html/Div",
                        "attributes": {
                            "style": {
                                "display": "flex",
                                "flexDirection": "row",
                                "flexWrap": "wrap",
                                "height": "50px"
                            }
                        },
                        "children": [
                            {
                                "id": "HomeButton",
                                "module": "material_ui/button/LinkButton",
                                "attributes": {
                                    "startIcon": "Home",
                                    "title": "Home",
                                    "route": "/${appName}/home",
                                    "style": {
                                        "height": "18px",
                                        "marginTop": "5px",
                                        "marginLeft": "30px",
                                        "color": "#1124B8",
                                        "background": "#e0e0e0",
                                        "borderRadius": "9px",
                                        "textTransform": "none",
                                        "fontSize": "16px"
                                    }
                                }
                            },
                            {
                                "id": "FastRewindButton",
                                "module": "material_ui/button/LinkButton",
                                "attributes": {
                                    "startIcon": "FastRewindTwoTone",
                                    "title": "Fast Rewind",
                                    "route": "xxxx",
                                    "style": {
                                        "height": "18px",
                                        "marginTop": "5px",
                                        "marginLeft": "30px",
                                        "color": "#31a4B8",
                                        "background": "#e0e0e0",
                                        "borderRadius": "9px",
                                        "textTransform": "none",
                                        "fontSize": "16px"
                                    }
                                }
                            },
                            {
                                "id": "PlayButton",
                                "module": "material_ui/button/LinkButton",
                                "attributes": {
                                    "title": "Play",
                                    "endIcon": "PlayArrowTwoTone",
                                    "route": "xxxx",
                                    "style": {
                                        "height": "18px",
                                        "marginTop": "5px",
                                        "marginLeft": "30px",
                                        "color": "#7124B8",
                                        "background": "#e0e0e0",
                                        "borderRadius": "9px",
                                        "textTransform": "none",
                                        "fontSize": "16px"
                                    }
                                }
                            },
                            {
                                "id": "FastForward",
                                "module": "material_ui/button/LinkButton",
                                "attributes": {
                                    "title": "Fast Forward",
                                    "endIcon": "FastForwardTwoTone",
                                    "route": "xxxx",
                                    "style": {
                                        "height": "18px",
                                        "marginTop": "5px",
                                        "marginLeft": "30px",
                                        "color": "#a124B8",
                                        "background": "#e0e0e0",
                                        "borderRadius": "9px",
                                        "textTransform": "none",
                                        "fontSize": "16px"
                                    }
                                }
                            },
                            {
                                "id": "Save",
                                "module": "material_ui/button/LinkButton",
                                "attributes": {
                                    "title": "Save",
                                    "startIcon": "SaveTwoTone",
                                    "route": "xxxx",
                                    "style": {
                                        "height": "18px",
                                        "marginTop": "5px",
                                        "marginLeft": "30px",
                                        "color": "#a1a428",
                                        "background": "#e0e0e0",
                                        "borderRadius": "9px",
                                        "textTransform": "none",
                                        "fontSize": "16px"
                                    }
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
}