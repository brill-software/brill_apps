{
    "pageDescrip": "Div Component",
    "permission": "cms_user",
    "rootComponent": {
        "id": "ErrorBoundary",
        "module": "react/ErrorBoundary",
        "attributes": {},
        "children": [
            {
                "id": "PageDiv",
                "module": "html/Div",
                "attributes": {
                    "style": {
                        "minHeight": "100vh",
                        "padding": "20px"
                    }
                },
                "children": [
                    {
                        "id": "Module",
                        "module": "material_ui/text/Typography",
                        "attributes": {
                            "variant": "h3",
                            "gutterBottom": true,
                            "text": "material_ui/icon/CustomIcon"
                        }
                    },
                    {
                        "id": "Attributes",
                        "module": "material_ui/text/Xhtml",
                        "attributes": {
                            "text": "<p>\n<table cellpadding=\"7\">\n<tr><td>subscribeToTopic</td><td>The topic for the SVG file of the icon. e.g. /Icons/custom/people.svg</td></tr>\n</table>\n</p>"
                        }
                    },
                    {
                        "id": "Custom Icon",
                        "module": "material_ui/icon/CustomIcon",
                        "attributes": {
                            "subscribeToTopic": "/Icons/custom/people.svg"
                        }
                    },
                    {
                        "id": "Notes",
                        "module": "material_ui/text/Xhtml",
                        "attributes": {
                            "text": "<p>The Gold SVG Icon was obtained from https://reactsvgicons.com.\n</p>"
                        }
                    },
                    {
                        "id": "Gold SVG Icon",
                        "module": "material_ui/icon/CustomIcon",
                        "attributes": {
                            "subscribeToTopic": "/Icons/custom/Gold.svg",
                            "style": {
                                "color": "gold"
                            }
                        }
                    }
                ]
            }
        ]
    }
}