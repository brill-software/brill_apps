                                    "style": {
                                        "display": "flex",
                                        "flexDirection": "row"
                                    }
                                },
                                "children": [
                                    {
                                        "id": "Flex Column Layout-1",
                                        "module": "html/Div",
                                        "attributes": {
                                            "style": {
                                                "display": "flex",
                                                "flexDirection": "column",
                                                "margin": "10px"
                                            }
                                        },
                                        "children": [
                                            {
                                                "id": "Text",
                                                "module": "material_ui/text/Xhtml",
                                                "attributes": {
                                                    "style": {
                                                        "fontSize": "20px",
                                                        "paddingTop": "10px",
                                                        "paddingBottom": "5px"
                                                    },
                                                    "text": "<p>Format <code>YYYY-MM-DD</code></p>"
                                                }
                                            },
