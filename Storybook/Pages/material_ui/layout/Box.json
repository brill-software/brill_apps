{
    "pageDescrip": "Boxr",
    "permission": "cms_user",
    "rootComponent": {
        "id": "ErrorBoundary",
        "module": "react/ErrorBoundary",
        "attributes": {},
        "children": [
            {
                "id": "PageDiv",
                "module": "html/Div",
                "attributes": {
                    "style": {
                        "minHeight": "100vh",
                        "padding": "20px"
                    }
                },
                "children": [
                    {
                        "id": "Module",
                        "module": "material_ui/text/Typography",
                        "attributes": {
                            "variant": "h3",
                            "gutterBottom": true,
                            "text": "material_ui/layout/Box"
                        }
                    },
                    {
                        "id": "Attributes",
                        "module": "material_ui/text/Xhtml",
                        "attributes": {
                            "text": "<p>\nBox component, based on the <a href=\"https://v4.mui.com/components/box/\" target=\"_blank\">Material-UI Box</a> component.\n</p>"
                        }
                    },
                    {
                        "id": "Box",
                        "module": "material_ui/layout/Box",
                        "attributes": {
                            "sx": {
                                "bgcolor": "secondary.main"
                            }
                        },
                        "children": [
                            {
                                "id": "PaperContent",
                                "module": "material_ui/text/Typography",
                                "attributes": {
                                    "variant": "subtitle1",
                                    "gutterBottom": true,
                                    "text": "This is the box content",
                                    "style": {
                                        "padding": "20px"
                                    }
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
}