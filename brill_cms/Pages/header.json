{
    "pageDescrip": "Header",
    "rootComponent": {
        "id": "_headerBox",
        "module": "html/Div",
        "attributes": {
            "style": {
                "key": "_headerBox",
                "width": "100%",
                "height": "41px",
                "background": "#1124B8",
                "border": "20px",
                "display": "flex",
                "flexDirection": "row"
            }
        },
        "children": [
            {
                "id": "panel1",
                "module": "material_ui/text/Typography",
                "attributes": {
                    "key": "_panel1",
                    "variant": "h1",
                    "style": {
                        "color": "#909090",
                        "paddingLeft": "12px",
                        "paddingRight": "40px",
                        "whiteSpace": "nowrap",
                        "fontSize": "2rem"
                    },
                    "text": "Brill CMS"
                }
            },
            {
                "id": "homeButton",
                "module": "material_ui/button/MenuButton",
                "attributes": {
                    "key": "_homeButton",
                    "title": "Home",
                    "route": "/brill_cms/home"
                }
            },
            {
                "id": "divider1",
                "module": "material_ui/text/Typography",
                "attributes": {
                    "key": "_divider1",
                    "variant": "body1",
                    "style": {
                        "color": "white",
                        "padding": "7px 8px"
                    },
                    "text": " | "
                }
            },
            {
                "id": "edit",
                "module": "material_ui/button/MenuButton",
                "attributes": {
                    "key": "_editButton",
                    "title": "Editor",
                    "route": "/brill_cms/editor"
                }
            },
            {
                "id": "_divider3",
                "module": "material_ui/text/Typography",
                "attributes": {
                    "key": "_divider3",
                    "variant": "body1",
                    "style": {
                        "color": "white",
                        "padding": "7px 8px"
                    },
                    "text": " | "
                }
            },
            {
                "id": "admin",
                "module": "material_ui/button/MenuButton",
                "attributes": {
                    "key": "_adminButton",
                    "title": "Admin",
                    "route": "/brill_cms/admin"
                }
            },
            {
                "id": "_divider6",
                "module": "material_ui/text/Typography",
                "attributes": {
                    "key": "_divider6",
                    "variant": "body1",
                    "style": {
                        "color": "white",
                        "padding": "7px 8px"
                    },
                    "text": " | "
                }
            },
            {
                "id": "help",
                "module": "material_ui/button/MenuButton",
                "attributes": {
                    "key": "_helpButton",
                    "title": "Help",
                    "route": "/brill_cms/help"
                }
            },
            {
                "id": "SwitchLightDark",
                "module": "material_ui/button/ThemeSwitchButton",
                "attributes": {
                    "setDarkToolTip": "Set dark background.",
                    "setLightToolTip": "Set light background.",
                    "style": {
                        "color": "#909090",
                        "marginLeft": "30px"
                    }
                }
            },
            {
                "id": "div",
                "module": "html/Div",
                "attributes": {
                    "variant": "body1",
                    "style": {
                        "key": "_div1",
                        "position": "absolute",
                        "right": "12px",
                        "display": "flex",
                        "flexDirection": "row",
                        "background": "#1124B8",
                        "paddingTop": "5px"
                    }
                },
                "children": [
                    {
                        "id": "username",
                        "module": "material_ui/text/Typography",
                        "attributes": {
                            "variant": "body1",
                            "style": {
                                "color": "white",
                                "marginRight": "40px"
                            },
                            "subscribeToTopic": "username"
                        }
                    },
                    {
                        "id": "logoutButton",
                        "module": "material_ui/button/LogoutButton",
                        "attributes": {
                            "key": "_logoutButton",
                            "title": "Logout",
                            "route": "/brill_cms/login",
                            "style": {
                                "height": "18px",
                                "marginTop": "5px",
                                "color": "#1124B8",
                                "background": "#e0e0e0",
                                "borderRadius": "9px",
                                "textTransform": "none"
                            },
                            "publishToTopic": "#logout"
                        }
                    }
                ]
            }
        ]
    }
}