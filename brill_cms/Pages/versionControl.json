{
    "pageDescrip": "Version Control Page",
    "rootComponent": {
        "id": "ErrorBoundary",
        "module": "react/ErrorBoundary",
        "attributes": {
            "key": "errorBoundary"
        },
        "children": [
            {
                "id": "ThemeProvider",
                "module": "material_ui/theme/ThemeProvider",
                "attributes": {
                    "key": "themeProvider",
                    "style": {
                        "minHeight": "100vh"
                    },
                    "themeTopic": "/Themes/brill_cms_theme_light.json",
                    "themeTopicDark": "/Themes/brill_cms_theme_dark.json"
                },
                "children": [
                    {
                        "id": "Padding",
                        "module": "html/Div",
                        "attributes": {
                            "style": {
                                "padding": "20px"
                            }
                        },
                        "children": [
                            {
                                "id": "Dialog Title-1",
                                "module": "material_ui/dialog/DialogContentText",
                                "attributes": {
                                    "text": "<b>Version Control Summary</b>"
                                }
                            },
                            {
                                "id": "Flex Column Layout-1",
                                "module": "html/Div",
                                "attributes": {
                                    "style": {
                                        "display": "flex",
                                        "flexDirection": "column"
                                    }
                                },
                                "children": [
                                    {
                                        "id": "Row-3",
                                        "module": "html/Div",
                                        "attributes": {
                                            "style": {
                                                "marginTop": "10px",
                                                "display": "flex",
                                                "flexDirection": "row"
                                            }
                                        },
                                        "children": [
                                            {
                                                "id": "Item-2",
                                                "module": "html/Div",
                                                "attributes": {
                                                    "text": "Workspace: ",
                                                    "style": {
                                                        "marginRight": "10px",
                                                        "marginBottom": "10px",
                                                        "fontSize": "20px",
                                                        "fontFamily": "Arial, sans-serif",
                                                        "color": "#808080"
                                                    }
                                                }
                                            },
                                            {
                                                "id": "Item-1",
                                                "module": "html/Div",
                                                "attributes": {
                                                    "subscribeToTopic": "git:current_ws:/",
                                                    "style": {
                                                        "marginBottom": "10px",
                                                        "fontSize": "20px",
                                                        "fontFamily": "Arial, sans-serif",
                                                        "color": "#808080"
                                                    }
                                                }
                                            },
                                            {
                                                "id": "Item-3",
                                                "module": "html/Div",
                                                "attributes": {
                                                    "text": "Branch:  ",
                                                    "style": {
                                                        "marginLeft": "50px",
                                                        "marginBottom": "10px",
                                                        "fontSize": "20px",
                                                        "fontFamily": "Arial, sans-serif",
                                                        "color": "#808080"
                                                    }
                                                }
                                            },
                                            {
                                                "id": "Item-4",
                                                "module": "html/Div",
                                                "attributes": {
                                                    "subscribeToTopic": "git:branch:/",
                                                    "style": {
                                                        "marginLeft": "10px",
                                                        "marginBottom": "10px",
                                                        "fontSize": "20px",
                                                        "fontFamily": "Arial, sans-serif",
                                                        "color": "#808080"
                                                    }
                                                }
                                            }
                                        ]
                                    },
                                    {
                                        "id": "Row-1",
                                        "module": "html/Div",
                                        "attributes": {
                                            "style": {
                                                "marginTop": "10px",
                                                "display": "flex",
                                                "flexDirection": "row"
                                            }
                                        },
                                        "children": [
                                            {
                                                "id": "Item-2",
                                                "module": "html/Div",
                                                "attributes": {
                                                    "text": "Repository: ",
                                                    "style": {
                                                        "marginRight": "10px",
                                                        "marginBottom": "10px",
                                                        "fontSize": "20px",
                                                        "fontFamily": "Arial, sans-serif",
                                                        "color": "#808080"
                                                    }
                                                }
                                            },
                                            {
                                                "id": "Item-1",
                                                "module": "html/Div",
                                                "attributes": {
                                                    "subscribeToTopic": "git:uri:/",
                                                    "style": {
                                                        "marginBottom": "10px",
                                                        "fontSize": "20px",
                                                        "fontFamily": "Arial, sans-serif",
                                                        "color": "#808080"
                                                    }
                                                }
                                            }
                                        ]
                                    }
                                ]
                            },
                            {
                                "id": "Flex Row Layout",
                                "module": "html/Div",
                                "attributes": {
                                    "style": {
                                        "display": "flex",
                                        "flexDirection": "row",
                                        "flexWrap": "wrap",
                                        "marginBottom": "30px"
                                    }
                                },
                                "children": [
                                    {
                                        "id": "Form",
                                        "module": "material_ui/input/Form",
                                        "attributes": {
                                            "style": {
                                                "width": "450px"
                                            }
                                        },
                                        "children": [
                                            {
                                                "id": "Flex column layout",
                                                "module": "html/Div",
                                                "attributes": {
                                                    "style": {
                                                        "display": "flex",
                                                        "flexDirection": "column"
                                                    }
                                                },
                                                "children": [
                                                    {
                                                        "id": "Dialog Title-2",
                                                        "module": "material_ui/dialog/DialogContentText",
                                                        "attributes": {
                                                            "text": "Workspaces:"
                                                        }
                                                    },
                                                    {
                                                        "id": "Radio",
                                                        "module": "material_ui/input/RadioGroup",
                                                        "attributes": {
                                                            "color": "primary",
                                                            "subscribeToTopic": "config:/workspace",
                                                            "buttonsTopic": "javascript:/brill_cms/Scripts/workspaceButtons.js",
                                                            "publishToTopic": "versionControl.switchWS#workspace",
                                                            "validationRules": [
                                                                {
                                                                    "code": "return value.length === 0",
                                                                    "errorMsg": "Please select a workspace. "
                                                                }
                                                            ]
                                                        }
                                                    },
                                                    {
                                                        "id": "Submit",
                                                        "module": "material_ui/button/SubmitButton",
                                                        "attributes": {
                                                            "size": "small",
                                                            "title": "Set Workspace",
                                                            "variant": "contained",
                                                            "route": "/brill_cms/editor",
                                                            "color": "primary",
                                                            "subscribeToTopic": "versionControl.switchWS",
                                                            "requestTopic": "git:workspace:/",
                                                            "style": {
                                                                "width": "160px"
                                                            }
                                                        }
                                                    }
                                                ]
                                            }
                                        ]
                                    },
                                    {
                                        "id": "Form-1",
                                        "module": "material_ui/input/Form",
                                        "attributes": {
                                            "style": {
                                                "width": "450px"
                                            }
                                        },
                                        "children": [
                                            {
                                                "id": "Flex column layout",
                                                "module": "html/Div",
                                                "attributes": {
                                                    "style": {
                                                        "display": "flex",
                                                        "flexDirection": "column"
                                                    }
                                                },
                                                "children": [
                                                    {
                                                        "id": "Dialog Title-2",
                                                        "module": "material_ui/dialog/DialogContentText",
                                                        "attributes": {
                                                            "text": "Checked out branches in current workspace:"
                                                        }
                                                    },
                                                    {
                                                        "id": "Radio-1",
                                                        "module": "material_ui/input/RadioGroup",
                                                        "attributes": {
                                                            "color": "primary",
                                                            "subscribeToTopic": "git:branch:/",
                                                            "buttonsTopic": "git:branches:/",
                                                            "publishToTopic": "versionControl.switchBranch#newBranch",
                                                            "style": {
                                                                "width": "300px"
                                                            },
                                                            "validationRules": [
                                                                {
                                                                    "code": "return value.length === 0",
                                                                    "errorMsg": "Please select a branch. "
                                                                }
                                                            ]
                                                        }
                                                    },
                                                    {
                                                        "id": "Submit",
                                                        "module": "material_ui/button/SubmitButton",
                                                        "attributes": {
                                                            "size": "small",
                                                            "title": "Switch Branch",
                                                            "variant": "contained",
                                                            "route": "/brill_cms/editor",
                                                            "color": "primary",
                                                            "subscribeToTopic": "versionControl.switchBranch",
                                                            "requestTopic": "git:switchbranch:/",
                                                            "style": {
                                                                "width": "160px"
                                                            }
                                                        }
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                ]
                            },
                            {
                                "id": "Dialog Title-2",
                                "module": "material_ui/dialog/DialogContentText",
                                "attributes": {
                                    "text": "<p>Awaiting Merge, Rebase or Pull:</p>"
                                }
                            },
                            {
                                "id": "MergePullOrRebase",
                                "module": "material_ui/table/Table",
                                "attributes": {
                                    "size": "small",
                                    "initialState": "{\n    \"pagination\": {\n      \"paginationModel\": { \"page\": 0, \"pageSize\": 5 },\n    }\n  }\n ",
                                    "pageSizeOptions": [
                                        5,
                                        10
                                    ],
                                    "padding": "10px",
                                    "maxRows": 20,
                                    "subscribeToTopic": "git:log:/",
                                    "columns": [
                                        {
                                            "name": "commit",
                                            "label": "Commit"
                                        },
                                        {
                                            "name": "awaiting",
                                            "label": "Awaiting"
                                        },
                                        {
                                            "name": "branch",
                                            "label": "Branch"
                                        },
                                        {
                                            "name": "message",
                                            "label": "Message"
                                        },
                                        {
                                            "name": "author",
                                            "label": "Author"
                                        },
                                        {
                                            "name": "date",
                                            "label": "Date"
                                        }
                                    ]
                                }
                            },
                            {
                                "id": "Flex Row Layout-1",
                                "module": "html/Div",
                                "attributes": {
                                    "style": {
                                        "display": "flex",
                                        "flexDirection": "row",
                                        "flexWrap": "wrap",
                                        "padding": "20px"
                                    }
                                },
                                "children": [
                                    {
                                        "id": "Rebase from Repository",
                                        "module": "material_ui/button/SubmitButton",
                                        "attributes": {
                                            "style": {
                                                "width": "120px",
                                                "marginRight": "40px"
                                            },
                                            "title": "Rebase",
                                            "variant": "contained",
                                            "route": "/brill_cms/editor",
                                            "color": "primary",
                                            "subscribeToTopic": "pullbranch.form",
                                            "requestTopic": "git:rebase:/"
                                        }
                                    },
                                    {
                                        "id": "Pull from Repository-1",
                                        "module": "material_ui/button/SubmitButton",
                                        "attributes": {
                                            "style": {
                                                "width": "120px"
                                            },
                                            "title": "Pull",
                                            "variant": "contained",
                                            "route": "/brill_cms/editor",
                                            "color": "primary",
                                            "subscribeToTopic": "pullbranch.form",
                                            "requestTopic": "git:pull:/"
                                        }
                                    }
                                ]
                            },
                            {
                                "id": "Dialog Title-6",
                                "module": "material_ui/dialog/DialogContentText",
                                "attributes": {
                                    "text": "<p>To merge, delete or create a new branch, right click on the Workspace folder at the top of the view tree and use the context menu.</p>"
                                }
                            },
                            {
                                "id": "Dialog Title-5",
                                "module": "material_ui/dialog/DialogContentText",
                                "attributes": {
                                    "text": "<p>Click on the Workspace folder to re-display this page.</p>"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
}