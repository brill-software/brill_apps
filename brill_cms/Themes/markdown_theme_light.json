            "color": "#707070",
            "@media (min-width:700px)": {
                "fontSize": "1.15rem",
                "lineHeight": "1.8rem"
            }
        },
        "ol": {
            "fontFamily": "-apple-system, BlinkMacSystemFont, \"Segoe UI\", \"Roboto\", \"Noto Sans\", \"Ubuntu\", \"Droid Sans\", \"Helvetica Neue\", sans-serif;",
            "fontSize": "0.9rem",
            "color": "#707070",
            "@media (min-width:700px)": {
                "fontSize": "1.15rem",
                "lineHeight": "1.8rem"
            }
        },
        "img": {
            "maxWidth": "100%",
            "height": "auto",
            "width": "auto"
        }
